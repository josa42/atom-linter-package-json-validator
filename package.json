{
  "name": "linter-package-json-validator",
  "main": "./lib/init",
  "version": "1.0.1",
  "description": "Lint `package.json` on the fly using package-json-validator and validate dependencies using nsp",
  "repository": "https://github.com/josa42/atom-linter-package-json-validator",
  "author": "josa42",
  "contributors": [
      "jbinard"
  ],
  "license": "MIT",
  "bugs": "https://github.com/josa42/atom-linter-package-json-validator/issues",
  "keywords": [
    "package.json",
    "validator",
    "lint",
    "nsp",
    "security"
  ],
  "scripts": {
    "lint": "eslint ."
  },
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "package-json-validator": "^0.6.1",
    "nsp": "^2.6.1",
    "loophole": "^1.1.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^6.0.1",
    "eslint": "^2.10.0"
  }
}
